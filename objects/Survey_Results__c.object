    <fields>
        <fullName>Animal_Count_Update__c</fullName>
        <externalId>false</externalId>
        <formula>Question_Answer_ID__r.Animal_Count_Update__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
