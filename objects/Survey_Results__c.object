    </fields>
    <fields>
        <fullName>Animal_Type_Decode__c</fullName>
        <description>Inserts and Animal Type when one is not identified.  Example:  If he Animal = General in Digigraph, decode to the proper animal type.</description>
        <externalId>false</externalId>
        <formula>If(
 Question_Answer_ID__r.Name  =&quot;Q-0013&quot;,&quot;Horse&quot;,
If(
 Question_Answer_ID__r.Name  =&quot;Q-0060&quot;,&quot;Horse&quot;,
If(
 Question_Answer_ID__r.Name =&quot;Q-0014&quot;,&quot;Cattle&quot;,
if(
 Question_Answer_ID__r.Name  =&quot;Q-0015&quot;,&quot;Dairy&quot;,
if(
 Question_Answer_ID__r.Name  =&quot;Q-0016&quot;,&quot;Swine&quot;,
Text(Question_Answer_ID__r.Animal_Type__c ))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Animal Type (Decode)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Animal_Type_Indicator__c</fullName>
        <description>Indicator which identifies which questions from the Survey Results table should be inserted into the Survey Results Summary Table.</description>
        <externalId>false</externalId>
        <formula>if( 
Question_Answer_ID__r.Name =&quot;Q-0040&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0041&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0042&quot;,&quot;Yes&quot;,  
If( 
Question_Answer_ID__r.Name =&quot;Q-0078&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0015&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0016&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0059&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0060&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0070&quot;,&quot;Yes&quot;, 
if( 
Question_Answer_ID__r.Name =&quot;Q-0077&quot;,&quot;Yes&quot;,
&quot;No&quot; ))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Animal Type Indicator</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
