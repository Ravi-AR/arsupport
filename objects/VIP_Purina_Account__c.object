<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Holds the Account Id to associate Contacts that are being created for users (Hosts) in order to associate the Host (User) to a campaignMember.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AccountId__c</fullName>
        <externalId>false</externalId>
        <label>AccountId</label>
        <length>25</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Bad_Animal_Type_Values__c</fullName>
        <description>Exclude this animal type from calculating points and tons.</description>
        <externalId>false</externalId>
        <inlineHelpText>Animal types to exclude from calculating of annual points.</inlineHelpText>
        <label>Bad Animal Type Values</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Bad_Opportunity_Record_Types__c</fullName>
        <description>Record types to excluded when calculating annual points and tons.
Use the developer Name  
EX: Record Type label : General Opportunity  DeveloperName = General_Opportunity
Separate fields with commas.</description>
        <externalId>false</externalId>
        <inlineHelpText>Opportunity Record types to excluded when calculating annual points and tons.</inlineHelpText>
        <label>Bad Opportunity Record Types</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Contact_Influencer_Type__c</fullName>
        <description>List (CSV) of contact types to treat as influencer.  Any contact type in this list will be treated and taken down the influencer Farm VIP path.</description>
        <externalId>false</externalId>
        <inlineHelpText>List of contact types to take the &quot;Influencer Path&quot;.</inlineHelpText>
        <label>Contact Influencer Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Contact_Owner_ID__c</fullName>
        <description>The user id that will own the contacts being created when a host is attending an event.  NEEDS to be SYSTEM ADMINISTRATOR for visibility.</description>
        <externalId>false</externalId>
        <label>Contact Owner ID</label>
        <length>25</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contact_Record_Type__c</fullName>
        <description>Holds the Purina Sales Specialist Record Id for the contact.</description>
        <externalId>false</externalId>
        <label>Contact Record Type</label>
        <length>25</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>VIP Purina Account</label>
    <visibility>Protected</visibility>
</CustomObject>
