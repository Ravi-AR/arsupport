    <validationRules>
        <fullName>ASM_and_SS_Request_Amount</fullName>
        <active>true</active>
        <description>Sales Specialist and Area Sales Managers cannot update the Request Amount</description>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Standard&quot;,
OR(
$Profile.Name = &quot;Sales Specialist&quot;,
$Profile.Name = &quot;Area Sales Manager&quot;,
$Profile.Name = &quot;Inside Sales User&quot;),
ISCHANGED(Total_Spend__c))</errorConditionFormula>
        <errorDisplayField>Total_Spend__c</errorDisplayField>
        <errorMessage>Unable to update Request Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ASM_and_SS_Status</fullName>
        <active>true</active>
        <description>Sales Specialist and Area Sales Managers can only select &apos;In Progress&apos; or &apos;Rework&apos; for the Line Approval Status</description>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Standard&quot;,
OR(
$Profile.Name = &quot;Sales Specialist&quot;,
$Profile.Name = &quot;Area Sales Manager&quot;,
$Profile.Name = &quot;Insides Sales User&quot;),
NOT(ISPICKVAL(Approval_Status__c, &apos;In Progress&apos;)),
NOT(ISPICKVAL(Approval_Status__c, &apos;Rework&apos;)))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Invalid Status. Select either &apos;In Progress&apos; or &apos;Rework&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Balance_Adjustment_Line_Edit_Validation</fullName>
        <active>true</active>
        <description>Only System Admins and Sales Ops users can edit Balance Adjustments Line Items.</description>
        <errorConditionFormula>Expense__r.RecordType.DeveloperName = &apos;Balance_Adjustment&apos; &amp;&amp; 
$Profile.Name != &apos;Sales Operations&apos; &amp;&amp; 
$Profile.Name != &apos;System Administrator&apos;</errorConditionFormula>
        <errorMessage>Only Approved Users can edit Balance Adjustment Line Items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReWork_Reason_Required</fullName>
        <active>true</active>
        <description>If line item status is &apos;Rework&apos;, reason code is required.</description>
        <errorConditionFormula>ISPICKVAL( Approval_Status__c , &apos;Rework&apos;) &amp;&amp;
 ISPICKVAL( ReasonforRework__c , &apos;&apos;)</errorConditionFormula>
        <errorMessage>Rejection reason required for Rework line item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequestAmountMoreThanAvailableBalance</fullName>
        <active>false</active>
        <description>Total Reimbursement Amount can not exceed Available Balance</description>
        <errorConditionFormula>ExpenseAmount__c &gt; Expense__r.Selling_Support_Program_Year__r.Available_Balance__c</errorConditionFormula>
        <errorMessage>Total Reimbursement Amount can not exceed Available Balance</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
    <webLinks>
        <fullName>ApproveLineItem</fullName>
        <availability>online</availability>
        <description>Sets the line item status to &apos;Approved&apos;</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve Item</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/37.0/connection.js&quot;)}  
 var records = {!GETRECORDIDS($ObjectType.AMASellingSupportExpenseLineItem__c)};   
 var updateRecords = [];   
 if (records[0] == null)   
 {   
  alert(&quot;Please select at least one Reimbursement Line Item&quot;)   
 }   
 else   
 {   
  for (var i=0; i&lt;records.length; i++)   
  {   
   var li = new sforce.SObject(&quot;AMASellingSupportExpenseLineItem__c&quot;);   
   li.id = records[i];   
   li.Approval_Status__c = &quot;In Progress&quot;;   
   updateRecords.push(li);   
  }   
  result = sforce.connection.update(updateRecords);   
  location.reload(true); 
 }</url>
    </webLinks>
    <webLinks>
        <fullName>Back_to_Reimbursement_Request</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>&lt;&lt; Back to Reimbursement Request</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <url>/{!AMASellingSupportExpenseLineItem__c.ExpenseId__c}</url>
    </webLinks>
    <webLinks>
        <fullName>RejectItem</fullName>
        <availability>online</availability>
        <description>Sets the line item status to &apos;Rejected&apos;</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set As Rework Item</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/37.0/connection.js&quot;)} 
var records = {!GETRECORDIDS($ObjectType.AMASellingSupportExpenseLineItem__c)}; 
var updateRecords = []; 
if (records[0] == null) 
{ 
alert(&quot;Please select at least one Reimbursement Line Item&quot;) 
} 
else 
{ 
for (var i=0; i&lt;records.length; i++) 
{ 
var li = new sforce.SObject(&quot;AMASellingSupportExpenseLineItem__c&quot;); 
li.id = records[i]; 
li.Approval_Status__c = &quot;Rework&quot;; 
updateRecords.push(li); 
} 
result = sforce.connection.update(updateRecords); 
location.reload(true); 
}</url>
    </webLinks>
