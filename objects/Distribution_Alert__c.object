    <fields>
        <fullName>Status__c</fullName>
        <description>The status of Cooked Tubs for the location.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(text(Status_Description__c )),NULL,
IF(text(Status_Description__c)=&quot;Red&quot;, IMAGE(&quot;/img/samples/color_red.gif&quot;, &quot;red&quot;, 15, 15),
IF(text(Status_Description__c)=&quot;Green&quot; ,IMAGE(&quot;/img/samples/color_green.gif&quot;, &quot;green&quot;, 15, 15),
IF(text(Status_Description__c)=&quot;Yellow&quot;,IMAGE(&quot;/img/samples/color_yellow.gif&quot;, &quot;yellow&quot;, 15, 15),NULL))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
