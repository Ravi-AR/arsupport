    <validationRules>
        <fullName>Cooked_Tubs_No_Reason_Green_Status</fullName>
        <active>true</active>
        <description>If the record type is Cooked Tubs and that Status description is Green, there should be no value selected for &quot;Reason&quot;.</description>
        <errorConditionFormula>And( 
RecordType.Name = &quot;Cooked Tubs&quot;, 
text( Status_Description__c ) = &quot;Green&quot;, 
Not(ISBLANK( text(Reason__c )) )
)</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>There should not be a &quot;Reason&quot; when the status is Green.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cooked_Tubs_Reason_For_Red_Status</fullName>
        <active>true</active>
        <description>If the Record Type is Cooked Tubs and the Status Description is Red, a Reason is required.</description>
        <errorConditionFormula>And(
RecordType.Name = &quot;Cooked Tubs&quot;,
text( Status_Description__c ) = &quot;Red&quot;, 
ISBLANK( text(Reason__c ))
)</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>This field is required.</errorMessage>
    </validationRules>
