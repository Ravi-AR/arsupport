    <validationRules>
        <fullName>BalanceAdjustmentNegative</fullName>
        <active>true</active>
        <description>Balances cannot be a negative number  LPDCR 1772</description>
        <errorConditionFormula>Available_Balance__c &lt; 0</errorConditionFormula>
        <errorMessage>Balances cannot be a negative number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_Validation_for_Community</fullName>
        <active>false</active>
        <description>Dealer community users cannot edit the Selling Support Program Year name</description>
        <errorConditionFormula>NOT(ISPICKVAL($User.UserType, &apos;Standard&apos;)) &amp;&amp;
 NOT( ISNEW() )</errorConditionFormula>
        <errorMessage>Dealer community users cannot edit the Selling Support Program Year name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSPY_Acct_has_a_BB_SS_and_ASM_Approver</fullName>
        <active>true</active>
        <description>SSPY Account has a BB SS and ASM Approver before Requests can be created</description>
        <errorConditionFormula>ISBLANK(Account__r.Business_Builder_Reimbursement_Approver__c) || ISBLANK( Account__r.ASM_BB_Approver__c) &amp;&amp;  Program_Year_Allocation__c  &lt;&gt; NULL</errorConditionFormula>
        <errorMessage>To create a SSPY Record Business Builder Approvers (Sales Specialist or ASM) are required on the Account record.</errorMessage>
    </validationRules>
