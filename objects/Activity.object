    <fields>
        <fullName>Activity_Count_Assigned_To_LPS_SS__c</fullName>
        <description>If the activity is assigned to a Sales Specialist or Technical Sales Specialist, then 1 else 0 for the current year.</description>
        <externalId>false</externalId>
        <formula>If(text(Owner:User.Function__c )= &quot;Sales Specialist&quot;  || text(Owner:User.Function__c) = &quot;Technical Sales Spec&quot; &amp;&amp; Text(YEAR(ActivityDate))= text(Year(today())),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count Curr Yr || Assigned to SS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Curr_Yr_Assigned_LPS__c</fullName>
        <description>If the activity count is assigned an LPS, then 1 else 0.</description>
        <externalId>false</externalId>
        <formula>IF( Text( Owner:User.Function__c ) =&quot;LPS&quot;  &amp;&amp; Text(YEAR(ActivityDate))= text(Year(today())),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count Curr Yr || Assigned  LPS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Curr_Yr__c</fullName>
        <description>Activity count is based on the date of the activity.</description>
        <externalId>false</externalId>
        <formula>If(Text(YEAR(ActivityDate ))=text(Year(today())),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count Curr Yr</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Prev_Yr_Assigned_To_SS__c</fullName>
        <description>If the activity is assigned to a Sales Specialist or a Technical Sales Spec and the activity date is in the previous year, then 1 else 0.</description>
        <externalId>false</externalId>
        <formula>If(text(Owner:User.Function__c )= &quot;Sales Specialist&quot;  || text(Owner:User.Function__c) = &quot;Technical Sales Spec&quot; &amp;&amp; Text(YEAR(ActivityDate))= text(Year(today())-1),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count Prev Yr || Assigned to SS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Prev_Yr_Assigned_to_LPS__c</fullName>
        <description>If the activity owner is an LPS and the activity date is in the previous year, then 1 else 0.</description>
        <externalId>false</externalId>
        <formula>IF( Text(Owner:User.Function__c) =&quot;LPS&quot;  &amp;&amp; Text(YEAR(ActivityDate))= text(Year(today())-1),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count Prev Yr || Assigned LPS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Prev_Yr_YTD__c</fullName>
        <description>Counts that activities from prev year YTD so comparisons can be made through same time period this year.</description>
        <externalId>false</externalId>
        <formula>If(
ActivityDate &gt;= DATE(Year(Today())-1, 1, 1)  &amp;&amp; 
ActivityDate &lt;=  DATE(Year(Today())-1, Month(Today()), Day(today())),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Count || Prev Yr YTD</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Count_Prev_Yr__c</fullName>
        <description>The activity count for the previous year is based on the date of the activity.</description>
        <externalId>false</externalId>
